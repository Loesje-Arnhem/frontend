{
  "extends": ["stylelint-config-standard", "stylelint-config-standard-vue"],
  "rules": {
    "import-notation": null,
    "at-rule-no-unknown": [
      true,
      {
        "ignoreAtRules": ["mixin", "define-mixin"]
      }
    ],
    "selector-pseudo-class-no-unknown": [
      true,
      {
        "ignorePseudoClasses": ["global", "readonly", "deep"]
      }
    ]
  }
}
