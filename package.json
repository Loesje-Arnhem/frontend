    "body-scroll-lock": "^2.6.4",
    "cross-env": "^6.0.3",
    "css-paint-polyfill": "^1.0.3",
    "graphql-tag": "^2.10.1",
    "nuxt": "^2.10.0",
    "vue-i18n": "^8.15.0",
    "vue-youtube": "^1.4.0",
    "vuelidate": "^0.7.4",
    "vuex-persist": "^2.1.1"
